# Configuration Supabase pour 224Suite
# Obtenez ces valeurs depuis votre dashboard Supabase : https://supabase.com/dashboard

# URL de votre projet Supabase
REACT_APP_SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_URL=https://your-project-id.supabase.co

# Clé anonyme de votre projet Supabase
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_ANON_KEY=your-anon-key-here

# Clé de service (pour les opérations côté serveur)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Configuration de l'application
NODE_ENV=development
PORT=3000

# Configuration de l'API
REACT_APP_API_URL=http://localhost:3000/api

# Configuration des services externes (optionnels)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Configuration email (optionnel)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_app_password

# Configuration Stripe (optionnel)
STRIPE_PUBLISHABLE_KEY=pk_test_your_key
STRIPE_SECRET_KEY=sk_test_your_key
