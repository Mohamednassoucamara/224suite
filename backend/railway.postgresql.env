# Variables d'environnement Railway pour PostgreSQL
# Ces variables seront configurées dans l'interface Railway

# Configuration de la base de données PostgreSQL
DB_USER=postgres
DB_PASSWORD=your_postgresql_password
DB_NAME=224suite_prod
DB_HOST=your_postgresql_host.railway.app
DB_PORT=5432

# Configuration du serveur
NODE_ENV=production
PORT=5000

# Configuration CORS
FRONTEND_URL=https://224suite.vercel.app

# Configuration JWT
JWT_SECRET=your_secure_jwt_secret_here_minimum_32_characters
JWT_EXPIRE=24h

# Configuration de sécurité
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
